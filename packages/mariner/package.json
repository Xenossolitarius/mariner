{
  "name": "mariner",
  "version": "1.0.0",
  "description": "Mariner Framework",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.mjs"
    },
    "./navigator": {
      "types": "./dist/navigator/index.d.ts",
      "import": "./dist/navigator/index.mjs",
      "require": "./dist/navigator/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "navigator": [
        "./dist/navigator/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "dist/navigator"
  ],
  "scripts": {
    "start": "tsx ./src/cli/main.ts",
    "start:build": "tsx ./src/cli/main.ts build",
    "dev": "nodemon --config nodemon.server.json",
    "build": "unbuild",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@koa/cors": "5.0.0",
    "@koa/router": "^12.0.1",
    "@types/koa": "^2.15.0",
    "ajv": "^8.12.0",
    "boxen": "^7.1.1",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "connect": "^3.7.0",
    "esbuild": "^0.20.1",
    "glob": "^10.3.10",
    "inquirer": "^9.2.15",
    "koa": "^2.15.0",
    "koa-connect": "^2.1.0",
    "ora": "^8.0.1",
    "@rollup/pluginutils": "^5.1.0",
    "vite": "^5.1.4",
    "vite-plugin-css-injected-by-js": "^3.4.0"
  },
  "devDependencies": {
    "@commander-js/extra-typings": "^11.1.0",
    "@types/connect": "^3.4.38",
    "@types/glob": "^8.1.0",
    "@types/inquirer": "9.0.7",
    "@types/koa__cors": "5.0.0",
    "@types/react": "^18.2.58",
    "@types/react-dom": "^18.2.19",
    "vue": "^3.4.19"
  }
}
