{
  "name": "mariner-fe",
  "version": "1.0.1",
  "description": "Mariner Framework",
  "author": "Ivan Pešić",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Xenossolitarius/mariner.git"
  },
  "keywords": ["microfrontend", "federation", "mariner-fe"],
  "license": "MIT",
  "type": "module",
  "types": "./dist/index.d.ts",
  "bin": {
    "mariner": "./bin/index.mjs",
    "mariner-cli": "./bin/index.mjs"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.mjs"
    },
    "./navigator": {
      "types": "./dist/navigator/index.d.ts",
      "import": "./dist/navigator/index.mjs",
      "require": "./dist/navigator/index.mjs"
    },
    "./plugins": {
      "types": "./dist/server/plugins/index.d.ts",
      "import": "./dist/server/plugins/index.mjs",
      "require": "./dist/server/plugins/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "navigator": [
        "./dist/navigator/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "dist/navigator",
    "dist/cli",
    "dist/server"
  ],
  "scripts": {
    "start": "tsx ./src/cli/main.ts",
    "start:build": "tsx ./src/cli/main.ts build",
    "start:generate": "tsx ./src/cli/main.ts generate",
    "mariner-fe": "mariner-fe",
    "dev": "nodemon --config nodemon.server.json",
    "build": "unbuild",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@koa/cors": "5.0.0",
    "@koa/router": "^12.0.1",
    "@rollup/pluginutils": "^5.1.0",
    "@types/koa": "^2.15.0",
    "ajv": "^8.14.0",
    "boxen": "^7.1.1",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "connect": "^3.7.0",
    "esbuild": "^0.20.2",
    "glob": "^10.4.1",
    "inquirer": "^9.2.22",
    "koa": "^2.15.3",
    "koa-connect": "^2.1.0",
    "node-forge": "^1.3.1",
    "ora": "^8.0.1",
    "vite": "^5.2.11",
    "vite-plugin-css-injected-by-js": "^3.5.1",
    "vite-plugin-dts": "^3.9.1"
  },
  "devDependencies": {
    "@types/connect": "^3.4.38",
    "@types/glob": "^8.1.0",
    "@types/inquirer": "9.0.7",
    "@types/koa__cors": "5.0.0",
    "@types/node-forge": "1.3.11",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "vue": "^3.4.27"
  },
  "peerDependencies": {
    "vite": "^5.2.11",
    "defu": "^6.1.4"
  }
}
